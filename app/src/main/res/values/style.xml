<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="NewSearchBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/space</item>
        <item name="android:layout_marginBottom">@dimen/space</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/backgroundtextview</item>
        <item name="android:padding">10dp</item>
        <item name="android:drawableStart">@drawable/ic_search_18dp</item>
        <item name="android:drawablePadding">@dimen/space</item>
        <item name="android:hint">@string/search</item>
        <item name="android:textColor">#A6A6A6</item>
        <item name="android:textColorHint">#A6A6A6</item>
        <item name="android:maxLines">1</item>
        <item name="android:lines">1</item>
        <item name="android:ellipsize">end</item>>
    </style>

    <style name="TabLayoutStyle" parent="Base.Widget.Design.TabLayout">
        <item name="android:textSize">11sp</item>
        <item name="textAllCaps">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>